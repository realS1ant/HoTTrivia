    <script src="/socket.io/socket.io.js"></script>
    <script src="moment-with-locales.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script type="text/javascript">
        const socket = io();

        let inRound = false;
        let roundNum = 0;
        let roundEnd = 0;
        let playersLeft = 0;

        socket.on('newPlayer', (sessionId, email, name) => {
            addToUsersTable(sessionId, email, name);
        });

        socket.on('removePlayer', (sessionId) => {
            deleteByID(sessionId);
        });

        socket.on('allPlayers', (players) => {
            $('#users-table-body').children().remove();
            players.forEach(player => {
                if (!tableHas(player.sessionId)) addToUsersTable(player.sessionId, player.email, player.name);
            });
        });

        socket.on('startRound', (num, end) => {
            inRound = true;
            roundNum = num;
            document.getElementById('round-num').innerText = `Round ${num}`;
            roundEnd = moment(end);
            document.getElementById('time-identifier').style.color = '';
        });

        socket.on('playersLeft', tot => {
            document.getElementById('players-left').innerText = `${tot} Players Left`;
        });

        socket.on('error', message => {
            document.getElementById('err-msg').innerText = message;
            document.getElementById('err-modal').classList.add('is-active');
        });

        //request list of all players for table.
        socket.emit('sendPlayers');

        //Update Times
        setInterval(() => {
            if (inRound) {
                // let difference = roundEndTime.getSeconds() - new Date().getSeconds();
                let difference = roundEnd.diff(new Date(), 'seconds');
                if (difference <= 0) {
                    document.getElementById('time-identifier').innerText = `0s`;
                    inRound = false;
                    document.getElementById('time-identifier').style.color = 'red';
                } else document.getElementById('time-identifier').innerText = `${difference}s`;
            }
        }, 250);


        function addToUsersTable(sessionId, email, name) {
            let cell1 = document.createElement('td');
            let cell2 = document.createElement('td');
            let cell3 = document.createElement('td')
            cell1.innerText = email;
            cell2.innerText = name;
            cell3.innerHTML = '<button class="button is-danger" onclick="deleteUser(this);"><span class="icon is-small"><i class="fas fa-ban"></i></span></button>';
            let id = document.createElement('td');
            id.innerHTML = '<span class="icon is-small" title="' + sessionId + '"><i class="fas fa-user"></i></span>';

            var tr = document.createElement('tr');
            tr.append(id);
            tr.append(cell1);
            tr.append(cell2);
            tr.append(cell3);

            var tableBody = document.getElementById('users-table-body');
            tableBody.append(tr);
        }

        function playerCount(tot) {
            document.getElementById('players-left').innerText = `${tot} Players Left`;
        }

        function deleteUser(button) {
            deleteByID(button.parentElement.parentElement.firstChild.firstChild.title);
        }

        function deleteByID(sessionId) {
            socket.emit('banPlayer', sessionId);
            $("span[title='" + sessionId + "']").parents('td').parents('tr').remove();
        }

        function tableHas(email) {
            return $('#users-table td:contains(' + email + ')').length > 0;
        }

        $('#revive-button').on('click', () => {
            if (!inRound) socket.emit('reviveEliminated');
            else {
                document.getElementById('err-msg').innerText = 'Still in the round!';
                document.getElementById('err-modal').classList.add('is-active');
            }
        });

        $('#reveal-button').on('click', () => {
            if (!inRound) socket.emit('sendResults');
            else {
                document.getElementById('err-msg').innerText = 'Still in the round!';
                document.getElementById('err-modal').classList.add('is-active');
            }
        });

        $('#new-round-button').on('click', () => {
            if (!inRound) socket.emit('startRound');
            else {
                document.getElementById('err-msg').innerText = 'Still in the round!';
                document.getElementById('err-modal').classList.add('is-active');
            }
        })

        $('#5-btn').on('click', () => {
            $('#round-time-input').val(5);
            socket.emit('setDefaultRoundDuration', $('#round-time-input').val());
            document.getElementById('err-msg').innerText = 'Round Duration Set';
            document.getElementById('err-modal').classList.add('is-active');
        });
        $('#10-btn').on('click', () => {
            $('#round-time-input').val(10);
            socket.emit('setDefaultRoundDuration', $('#round-time-input').val());
            document.getElementById('err-msg').innerText = 'Round Duration Set';
            document.getElementById('err-modal').classList.add('is-active');
        });
        $('#15-btn').on('click', () => {
            $('#round-time-input').val(15);
            socket.emit('setDefaultRoundDuration', $('#round-time-input').val());
            document.getElementById('err-msg').innerText = 'Round Duration Set';
            document.getElementById('err-modal').classList.add('is-active');
        });
        $('#set-duration-btn').on('click', () => {
            socket.emit('setDefaultRoundDuration', $('#round-time-input').val());
            document.getElementById('err-msg').innerText = 'Round Duration Set';
            document.getElementById('err-modal').classList.add('is-active');
        });
        $('#round-time-input').on('keydown', e => {
            if (e.keyCode === 13) {
                socket.emit('setDefaultRoundDuration', $('#round-time-input').val());
                this.blur();
                document.getElementById('err-msg').innerText = 'Round Duration Set';
                document.getElementById('err-modal').classList.add('is-active');
            }
        });

        function updateLoginSettings() {
            console.log('updating...')
            socket.emit('updateLoginSettings', $('#allow-players-check').prop('checked'), $('#yes').prop('checked'));
        }

        $('#allow-players-check').on('click', updateLoginSettings);
        $('#yes').on('click', updateLoginSettings);
        $('#no').on('click', updateLoginSettings);

        $('#update-login-btn').on('click', () => {
            socket.emit('updateLoginSettings', $('#allow-players-check').prop('checked'), $('#yes').prop('checked'));
        });

        $('#err-modal #close-btn').on('click', () => {
            $('#err-modal').removeClass('is-active');
        });
        $('#err-modal #x-btn').on('click', () => {
            $('#err-modal').removeClass('is-active');
        });

        $('#round-num-btn').on('click', () => {
            socket.emit('nextRndNum', ($('#round-num-input').val() - 1));
        });

        $('#presentation-controls #pre-game').on('click', () => {
            $('#confirm-modal #confirm-action-msg').text('go to pre-game view');
            $('#confirm-modal').addClass('is-active');
        });

        $('#presentation-controls #playing').on('click', () => {
            socket.emit('setPresentationView', 'playing');
        });

        $('#presentation-controls #game-over').on('click', () => {
            if (inRound) {
                document.getElementById('err-msg').innerText = 'Still in a round!';
                document.getElementById('err-modal').classList.add('is-active');
                return;
            }
            $('#confirm-modal #confirm-action-msg').text('end the game');
            $('#confirm-modal').addClass('is-active');
        });

        $('#confirm-modal #confirm-btn').on('click', () => {
            $('#confirm-modal').removeClass('is-active');
            if ($('#confirm-modal #confirm-action-msg').text() === 'end the game') socket.emit('setPresentationView', 'game-over');
            else if ($('#confirm-modal #confirm-action-msg').text() === 'go to pre-game view') socket.emit('setPresentationView', 'pre-game');
        });
        $('#confirm-modal #cancel-btn').on('click', () => {
            $('#confirm-modal').removeClass('is-active');
        });
        $('#confirm-modal #x-btn').on('click', () => {
            $('#confirm-modal').removeClass('is-active');
        });

        document.getElementById('logout-button').onclick = () => {
            axios.post('/api/admin/logout').then(res => {
                if (res.data.success) window.location = res.data.redirectTo;
                else alert('Server Error');
            });
        };

        $('#box-height-btn').on('click', () => {
            socket.emit('boxHeight', $('#box-height-input').val());
        });

        $('#csv-file-name').hide();
        $('#csv-file-submit').hide();
        $('#csv-file-input').on('change', function () {
            const name = $('#csv-file-input')[0].files[0].name;
            document.getElementById('csv-file-name').innerHTML = name;
            if (name.length !== 0) {
                $('#csv-file-name').show();
                $('#csv-file-submit').show();
            } else {
                $('#csv-file-name').hide();
                $('#csv-file-submit').hide();
            }
        });
        $('#csv-file-form').on('submit', function () {
            document.getElementById('csv-file-info').innerHTML = 'Submitted CSV.';
        });

        $('#email-search-input').on('keydown', e => {
            if (e.keyCode === 13) {
                this.blur();
                $('#lookup-content').html('');
                axios.post('/api/v1/search', { email: $('#email-search-input').val().toLowerCase() }).then(res => {
                    if (res.data.success) {
                        if (res.data.error) {
                            $('#lookup-content').html(`<h1 class="p-3" style="color: red; font-size: 2rem;">${error}</h1>`);
                            return;
                        }
                        if (res.data.uses === 0 && !res.data.accounts) {
                            //not in either collection
                            $('#lookup-content').html(` 
                                <h1 style="font-size: 2rem; color: red;" class="p-3">Email not in system.</h1>
                                <div class="columns p-3">
                                 <div class="column">
                                    <h1 style="font-size: 1.5rem;">${res.data.email}</h1>
                                 </div>
                                 <div class="column">
                                     <button class="button is-primary is-large" onclick="addUse('${res.data.email}');">Add Use</button>
                                 </div>
                             </div>
                            `);
                            $('#lookup-content').html('<h1 style="color: red; font-size: 2rem;" class="p-3">Email not in System.</h1>');
                            return;
                        } else if (res.data.uses >= 1 && !res.data.accounts) {
                            //No accounts made, but email preset in system.
                            $('#lookup-content').html(`<h1 style="font-size: 2rem;" class="p-3">${res.data.email}</h1>
                             <div class="columns p-3">
                                 <div class="column">
                                     <h1 style="font-size: 2rem;"><b>Uses: </b> <span id="uses">${res.data.uses}</span></h1>
                                 </div>
                                 <div class="column">
                                     <button class="button is-primary is-large" onclick="addUse('${res.data.email}');">Add Use</button>
                                 </div>
                             </div>`);
                        } else if (res.data.uses === 0 && res.data.accounts != []) {
                            //No more uses left, but an account is made.
                            let inner = `<h1 style="font-size: 2rem;" class="p-3">${res.data.email}</h1>
                             <div class="columns p-3">
                                 <div class="column">
                                     <h1 style="font-size: 2rem;"><b>Uses: </b> <span id="uses">${res.data.uses}</span></h1>
                                 </div>
                                 <div class="column">
                                     <button class="button is-primary is-large" onclick="addUse('${res.data.email}');">Add Use</button>
                                 </div>
                             </div>`;

                            for (var acc of res.data.accounts) {
                                inner += `<div class="columns p-3">
                                                 <div class="column">
                                                     <ul style='font-size: 1.75rem;'>
                                                         <li><b>Name: </b><span id="acc-name">${acc.name}</span></li>
                                                         <li><b>Email: </b><span id="acc-email">${acc.email}</span></li>
                                                     </ul>
                                                 </div>
                                                 <div class="column">
                                                     <div class="columns stack">
                                                         <div class="column"></div>
                                                         <div class="column"><button class="button is-danger is-large"
                                                                 onclick="banSesh('${acc.sessionId}', this);">Ban</button></div>
                                                     </div>
                                                 </div>
                                             </div>`;
                            }
                            $('#lookup-content').html(inner);
                        } else {
                            $('#lookup-content').html('<h1 style="color: red; font-size: 2rem;" class="p-3">Internal Error.</h1>');
                        }
                    } else {
                        $('#lookup-content').html('<h1 style="color: red; font-size: 2rem;">Server Error</h1>');
                    }
                });
            }
        });

        function addUse(email) {
            axios.post('/api/v1/addUse', { email }).then(res => {
                if (res.data.success) {
                    if (res.data.error) {
                        document.getElementById('err-msg').innerText = res.data.error;
                        document.getElementById('err-modal').classList.add('is-active');
                        return;
                    }
                    $('#uses').text(res.data.newUses);
                } else {
                    document.getElementById('err-msg').innerText = 'Internal Server Error';
                    document.getElementById('err-modal').classList.add('is-active');
                }
            });
        }

        function banSesh(sid, element) {
            socket.emit('banPlayer', sid);
            let rm = element.parentElement.parentElement.parentElement;
            rm.parentElement.remove(rm);
        }

    </script>
</body>

</html>